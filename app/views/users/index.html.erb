<h1>Users index</h1>
<ul class="media-list">
  <% @users.each do |user| %>
  	<li class="media">
  		<h4 class="media-left" style="color: blue;">
  			USER:
  		</h4>
  		<div class="media-body">
  			<h4 class="media-heading"><%= user.name %></h4>
  			Registered: <%= user.created_at %>
  		</div>
  	</li>
  <% end %>
</ul>
